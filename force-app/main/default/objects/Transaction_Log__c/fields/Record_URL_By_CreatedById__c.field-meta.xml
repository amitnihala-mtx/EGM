<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Record_URL_By_CreatedById__c</fullName>
    <description>Formula field to generate URL on the basis of Created By user&apos;s org.</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK( CreatedBy.ContactId ),
 $Label.Core_NYSERDA_Internal_URL ,
 $Label.CORE_Branded_NYSERDA_Portal_URL 
)  &amp; Id</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Record URL By CreatedById</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
